{
  "name": "convertkit-node",
  "description": "Simple SDK built for Node.js to interact with the Convertkit API ",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --ext .ts src/",
    "lint-fix": "npm run lint -- --fix",
    "build": "tsc",
    "prepare": "npm run build",
    "test": "jest -c jest.config.unit.js",
    "test:integration": "jest -c jest.config.integration.js"
  },
  "keywords": [
    "convertkit"
  ],
  "author": "Joel Baudin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hightcoast/convertkit-node.git"
  },
  "bugs": {
    "url": "https://github.com/hightcoast/convertkit-node/issues"
  },
  "homepage": "https://github.com/hightcoast/convertkit-node#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write",
      "npm run lint-fix"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "lodash": "^4.17.15"
  }
}
